<Defs>
	<!--simple chains: lumber-->
	<ThingDef ParentName="BuildingBase">
		<defName>Owl_WoodDryingRack</defName>
		<label>wood drying rack</label>
		<description>A drying rack for green lumber.</description>
		<thingClass>ItemProcessor.Building_ItemProcessor</thingClass>
		<drawerType>MapMeshAndRealTime</drawerType>
		<graphicData>
			<texPath>Things/Building/Production/wood_drying_rack</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawSize>(2,2)</drawSize>
			<damageData>
				<rectN>(0.125,0.296875,0.734375,0.421875)</rectN>
				<rectS>(0.125,0.296875,0.734375,0.421875)</rectS>
				<rectE>(0.234375,0.296875,0.5,0.546875)</rectE>
				<rectW>(0.234375,0.296875,0.5,0.546875)</rectW>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
			<shadowData>
				<volume>(0.5, 0.5, 0.5)</volume>
				<offset>(0,0,-0.25)</offset>
			</shadowData>
		</graphicData>
		<uiIconPath>Things/Building/Production/wood_drying_rack_north</uiIconPath>
		<stuffCategories>
			<li>Metallic</li>
			<li>Woody</li>
			<li>RawWoody</li>
			<li>Stony</li>
		</stuffCategories>
		<costStuffCount>10</costStuffCount>
		<altitudeLayer>Building</altitudeLayer>
		<fillPercent>0.3</fillPercent>
		<castEdgeShadows>true</castEdgeShadows>
		<staticSunShadowHeight>0</staticSunShadowHeight>
		<useHitPoints>True</useHitPoints>
		<statBases>
			<Mass>4</Mass>
			<MaxHitPoints>75</MaxHitPoints>
			<WorkToBuild>500</WorkToBuild>
			<Flammability>1.0</Flammability>
		</statBases>
		<designationCategory>Production</designationCategory>
		<passability>PassThroughOnly</passability>
		<pathCost>70</pathCost>
		<modExtensions>
			<li MayRequire="owlchemist.perspectivebuildings" Class="Perspective.Offsetter" />
		</modExtensions>
		<comps>
			<li Class="ItemProcessor.CompProperties_ItemProcessor">
				<isSemiAutomaticMachine>true</isSemiAutomaticMachine>
				<chooseIngredientsIcon>Things/Item/Resource/Lumber/owl_lumberc</chooseIngredientsIcon>
				<bringIngredientsIcon>Things/Item/Resource/Lumber/owl_lumberc</bringIngredientsIcon>
				<cancelIngredientsIcon>Things/Item/Resource/Lumber/owl_lumberc</cancelIngredientsIcon>
				<resetSemiautomaticIcon>Things/Item/Resource/Lumber/owl_lumberc</resetSemiautomaticIcon>
				<buildingOnGraphic>Things/Building/Production/wood_drying_rack_full</buildingOnGraphic>
				<buildingFinishedGraphic>Things/Building/Production/wood_drying_rack_full</buildingFinishedGraphic>
				<showFactoryProgressBar>true</showFactoryProgressBar>
			</li>
		</comps>
		<tickerType>Rare</tickerType>
		<rotatable>false</rotatable>
		<minifiedDef>MinifiedThing</minifiedDef>
		<thingCategories>
			<li>BuildingsProduction</li>
		</thingCategories>
		<building>
			<uninstallWork>125</uninstallWork>
			<buildingTags>
				<li>Production</li>
			</buildingTags>
		</building>
	</ThingDef>
	<ThingDef ParentName="BuildingBase">
		<defName>Owl_CarpentersTrestle</defName>
		<label>carpentry trestle</label>
		<description>A pair of trestles can be used for basic woodworking tasks.</description>
		<thingClass>Building_WorkTable</thingClass>
		<graphicData>
			<texPath>Things/Building/Production/carpenters_trestle</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawSize>(2.9,1.1)</drawSize>
			<drawOffset>(0,0,0.1)</drawOffset>
		</graphicData>
		<uiIconPath>Things/Building/Production/carpenters_trestle_north</uiIconPath>
		<stuffCategories>
			<li>Metallic</li>
			<li>Woody</li>
			<li>RawWoody</li>
		</stuffCategories>
		<costStuffCount>20</costStuffCount>
		<costList>
			<Steel>20</Steel>
		</costList>
		<altitudeLayer>Building</altitudeLayer>
		<fillPercent>0.2</fillPercent>
		<useHitPoints>True</useHitPoints>
		<statBases>
			<Mass>3</Mass>
			<MaxHitPoints>100</MaxHitPoints>
			<WorkToBuild>2000</WorkToBuild>
			<Flammability>0.75</Flammability>
			<WorkTableWorkSpeedFactor>0.66</WorkTableWorkSpeedFactor>
		</statBases>
		<size>(3,1)</size>
		<designationCategory>Production</designationCategory>
		<passability>PassThroughOnly</passability>
		<pathCost>70</pathCost>
		<hasInteractionCell>True</hasInteractionCell>
		<interactionCellOffset>(0,0,-1)</interactionCellOffset>
		<surfaceType>Item</surfaceType>
		<inspectorTabs>
			<li>ITab_Bills</li>
		</inspectorTabs>
		<building>
			<spawnedConceptLearnOpportunity>BillsTab</spawnedConceptLearnOpportunity>
			<uninstallWork>500</uninstallWork>
			<buildingTags>
				<li>Production</li>
			</buildingTags>
			<relatedBuildCommands>
				<li>Owl_WoodDryingRack</li>
			</relatedBuildCommands>
		</building>
		<comps>
			<li Class="CompProperties_AffectedByFacilities">
				<linkableFacilities>
					<li>ToolCabinet</li>
				</linkableFacilities>
			</li>
		</comps>
		<placeWorkers>
			<li>PlaceWorker_ShowFacilitiesConnections</li>
		</placeWorkers>
		<minifiedDef>MinifiedThing</minifiedDef>
		<thingCategories>
			<li>BuildingsProduction</li>
		</thingCategories>
	</ThingDef>
	<!--simple chains: steel-->
	<ThingDef ParentName="BuildingBase">
		<defName>Owl_Bloomery</defName>
		<label>bloomery</label>
		<description>Used to refine steel, but this primitive design is both more labor intensive and less fuel efficient than a proper smelting station.</description>
		<thingClass>Building_WorkTable</thingClass>
		<graphicData>
			<texPath>Things/Building/Production/bloomery</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawSize>(1.25,1.25)</drawSize>
			<drawOffset>(0,0,0.3)</drawOffset>
			<damageData>
				<rectN>(0.125,0.296875,0.734375,0.421875)</rectN>
				<rectS>(0.125,0.296875,0.734375,0.421875)</rectS>
				<rectE>(0.234375,0.296875,0.5,0.546875)</rectE>
				<rectW>(0.234375,0.296875,0.5,0.546875)</rectW>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
			<shadowData>
				<volume>(0.5, 0.5, 0.5)</volume>
			</shadowData>
		</graphicData>
		<uiIconPath>Things/Building/Production/bloomery_north</uiIconPath>
		<stuffCategories>
			<li>Chunky</li>
		</stuffCategories>
		<costStuffCount>3</costStuffCount>
		<altitudeLayer>Building</altitudeLayer>
		<fillPercent>0.5</fillPercent>
		<castEdgeShadows>true</castEdgeShadows>
		<statBases>
			<Mass>10</Mass>
			<MaxHitPoints>250</MaxHitPoints>
			<WorkToBuild>1750</WorkToBuild>
			<Flammability>0</Flammability>
			<WorkTableWorkSpeedFactor>0.7</WorkTableWorkSpeedFactor>
			<Cleanliness>-3</Cleanliness>
		</statBases>
		<designationCategory>Production</designationCategory>
		<passability>PassThroughOnly</passability>
		<pathCost>70</pathCost>
		<hasInteractionCell>True</hasInteractionCell>
		<interactionCellOffset>(0,0,-1)</interactionCellOffset>
		<surfaceType>Item</surfaceType>
		<inspectorTabs>
			<li>ITab_Bills</li>
		</inspectorTabs>
		<building>
			<spawnedConceptLearnOpportunity>BillsTab</spawnedConceptLearnOpportunity>
		</building>
		<recipes>
			<li>ExtractMetalFromSlag</li>
			<li>SmeltWeapon</li>
			<li>SmeltApparel</li>
			<li>DestroyWeapon</li>
			<li>DestroyApparel</li>
			<li>BurnDrugs</li>
		</recipes>
		<comps>
			<li Class="CompProperties_Refuelable">
				<fuelConsumptionRate>80.0</fuelConsumptionRate>
				<fuelCapacity>50.0</fuelCapacity>
				<fuelFilter>
					<thingDefs>
						<li>WoodLog</li>
					</thingDefs>
				</fuelFilter>
				<consumeFuelOnlyWhenUsed>true</consumeFuelOnlyWhenUsed>
			</li>
			<li Class="CompProperties_HeatPusher">
				<compClass>CompHeatPusherPowered</compClass>
				<heatPerSecond>12</heatPerSecond>
			</li>
			<li MayRequire="owlchemist.simplefx.smoke2" Class="Flecker.CompProperties_Smoker">
				<fleckDef>Owl_SmokeHeavy</fleckDef>
				<billsOnly>true</billsOnly>
				<particleOffset>(0,0,0.5)</particleOffset>
			</li>
		</comps>
	</ThingDef>
	<!--fiber-->
